{
  "name": "vue-better-sync",
  "version": "3.1.1",
  "description": "Simply sync values.",
  "repository": {
    "type": "git",
    "url": "fjc0k/vue-better-sync"
  },
  "author": "fjc0k <fjc0kb@gmail.com>",
  "license": "MIT",
  "main": "dist/vue-better-sync.cjs.js",
  "module": "dist/vue-better-sync.es.js",
  "unpkg": "dist/vue-better-sync.min.js",
  "jsdelivr": "dist/vue-better-sync.min.js",
  "files": [
    "dist"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "scripts": {
    "test": "cross-env BABEL_ENV=test jest",
    "test:cov": "cross-env BABEL_ENV=test jest --coverage",
    "build": "bili --format es,cjs,umd,umd-min --module-name VueBetterSync --js buble --banner --no-babel.babelrc"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@fir-ui/fir"
  },
  "devDependencies": {
    "@fir-ui/eslint-config-fir": "^0.3.0",
    "@vue/test-utils": "^1.0.0-beta.12",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "bili": "^3.0.7",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "jest": "^22.4.2",
    "vue": "^2.5.16",
    "vue-jest": "^2.1.1",
    "vue-template-compiler": "^2.5.16"
  }
}
